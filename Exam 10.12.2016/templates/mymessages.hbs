
{{>boxespartial}}

<section id="viewMyMessages">
            <h1>My Messages</h1>
            <div class="messages" id="myMessages">
                <table>
                    <thead>
                        <tr>
                            <th>From</th>
                            <th>Message</th>
                            <th>Date Received</th>
                        </tr>
                    </thead>
                    <tbody>
                      {{#each data}}
                        <tr>
                            <td>{{name}}</td>
                            <td>{{text}}</td>
                            <td>{{date}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </section>